
visible this [
    count = 0
]

visible _2nd [
    some = nil
]

label visit[this, _2nd] println()
{
    print(src= count)
    some = count
}

label visit[_2nd] dothis() {
    print(src=some)
}

label visit[this] main()
{
    count++
    count++
    count++
}
main()
